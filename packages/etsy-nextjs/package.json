{
  "name": "@profplum700/etsy-nextjs",
  "version": "2.3.1",
  "type": "module",
  "description": "Next.js integration for Etsy v3 API client",
  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c --watch",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint --config ../../eslint.config.mjs .",
    "lint:fix": "eslint --fix --config ../../eslint.config.mjs .",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "pnpm run build && pnpm run test"
  },
  "keywords": [
    "etsy",
    "api",
    "v3",
    "nextjs",
    "next.js",
    "react",
    "typescript",
    "javascript"
  ],
  "author": {
    "name": "profplum700",
    "email": "profplum700@users.noreply.github.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/profplum700/etsy-v3-api-client.git",
    "directory": "packages/etsy-nextjs"
  },
  "bugs": {
    "url": "https://github.com/profplum700/etsy-v3-api-client/issues"
  },
  "homepage": "https://github.com/profplum700/etsy-v3-api-client/tree/main/packages/etsy-nextjs#readme",
  "engines": {
    "node": ">=20.0.0"
  },
  "peerDependencies": {
    "@profplum700/etsy-v3-api-client": "^2.0.0",
    "next": "^14.0.0 || ^15.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@profplum700/etsy-v3-api-client": "workspace:*",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@types/react": "^18.3.18",
    "jest": "^30.0.5",
    "next": "^15.1.4",
    "react": "^18.3.1",
    "rollup": "^4.45.1",
    "rollup-plugin-dts": "^6.1.1",
    "ts-jest": "^29.4.0",
    "typescript": "^5.5.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
